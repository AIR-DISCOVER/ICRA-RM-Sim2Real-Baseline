<?xml version="1.0"?>
<launch>
 <param name="use_sim_time" value="false" />

<!-- <param name="robot_description" textfile="$(find carto_navigation)/urdf/model.urdf"/>
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" /> -->
  <arg name="configuration_basename" value="cartographer_2d.lua"/>
  <node name="cartographer_node" pkg="cartographer_ros"
      type="cartographer_node" args="
          -configuration_directory
              $(find carto_navigation)/param
          -configuration_basename $(arg configuration_basename)"
      >
    <remap from="scan" to="/scan_multi"/>

    <remap from="imu" to="/imu/data_raw"/>
    <remap from="odom" to="/ep/odom"/>
  </node>
  <node name="cartographer_occupancy_grid_node" pkg="cartographer_ros" type="cartographer_occupancy_grid_node" args="-resolution 0.05" >
    <remap from="map" to="map"/>
  </node>
</launch>
